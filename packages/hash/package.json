{
  "name": "@httpx/hash",
  "description": "Collection of hash functions",
  "version": "0.2.4",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "homepage": "https://belgattitude.github.io/httpx/hash",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/httpx.git",
    "directory": "packages/hash"
  },
  "keywords": [
    "hash",
    "md5",
    "spark"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./xxhash-wasm": {
      "types": "./dist/xxhash-wasm/index.d.ts",
      "default": "./dist/xxhash-wasm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "?build-release": "When https://github.com/atlassian/changesets/issues/432 has a solution we can remove this trick",
    "build": "run clean && yarn run tsup",
    "build-release": "run build",
    "bench": "vitest bench --run",
    "bench-bun": "bun --bun run vitest bench --run",
    "bench-codspeed": "cross-env CODSPEED=1 vitest bench --run",
    "bench-watch": "vitest bench",
    "docgen": "run-s docgen-typedoc",
    "docgen-typedoc": "rimraf ./docs/api && typedoc --plugin typedoc-plugin-markdown --out ./docs/api",
    "check-dist": "es-check es2022 './dist/**/*.js' --not './dist/*.map.js' --module",
    "check-pub": "attw --pack --profile esm-only && publint",
    "check-size": "size-limit",
    "clean": "rimraf ./dist ./build ./coverage ./_release",
    "dev": "tsup --watch",
    "fix-staged": "lint-staged --allow-empty",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/hash.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-bun": "bun --bun run vitest run",
    "test-unit-coverage": "vitest run --coverage",
    "test-unit-edge": "vitest run --environment edge-runtime",
    "test-unit-browser": "vitest run --browser",
    "test-unit-cloudflare": "vitest run --config ./vitest.cloudflare.config.ts",
    "test-unit-watch": "vitest --ui --api 4515",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-build": "tsc --project tsconfig.build.json --noEmit",
    "ci-coverage-upload": "../../.github/scripts/download/codecov -F httpx-hash-unit --dir ./coverage"
  },
  "peerDependencies": {
    "xxhash-wasm": "^1.1.0"
  },
  "peerDependenciesMeta": {
    "xxhash-wasm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@belgattitude/eslint-config-bases": "8.7.1",
    "@httpx/devtools-vitest": "workspace:^",
    "@size-limit/file": "12.0.0",
    "@size-limit/webpack": "12.0.0",
    "@types/lodash-es": "4.17.12",
    "browserslist": "4.28.0",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "10.1.0",
    "es-check": "9.5.0",
    "esbuild": "0.27.1",
    "eslint": "8.57.1",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "publint": "0.3.15",
    "rimraf": "6.1.2",
    "size-limit": "12.0.0",
    "tsup": "8.5.1",
    "typedoc": "0.28.15",
    "typedoc-plugin-markdown": "4.9.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15",
    "webpack": "5.103.0",
    "xxhash-wasm": "1.1.0"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
