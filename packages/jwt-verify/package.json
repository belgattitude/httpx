{
  "name": "@httpx/jwt-verify",
  "description": "Verify JSON Web Tokens (JWT)",
  "version": "0.2.0",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "homepage": "https://belgattitude.github.io/httpx/jwt-verify",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/httpx.git",
    "directory": "packages/jwt-verify"
  },
  "keywords": [
    "jsonwebtoken",
    "jwt-verify",
    "jwt",
    "openid",
    "entra",
    "azure-ad",
    "lru",
    "standard schema"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "?build-release": "When https://github.com/atlassian/changesets/issues/432 has a solution we can remove this trick",
    "build": "tsdown",
    "build-release": "yarn build && rimraf ./_release && yarn pack && mkdir ./_release && tar zxvf ./package.tgz --directory ./_release && rm ./package.tgz",
    "bench-disabled": "vitest bench --run",
    "bench-bun-disabled": "bun --bun run vitest bench --run",
    "bench-codspeed-disabled": "cross-env CODSPEED=1 vitest bench --run",
    "bench-watch-disabled": "vitest bench",
    "docgen": "run-s docgen-typedoc",
    "docgen-typedoc": "rimraf ./docs/api && typedoc --plugin typedoc-plugin-markdown --out ./docs/api",
    "check-dist": "es-check es2022 './dist/**/*.js' --not './dist/*.map.js' --module",
    "check-pub": "attw --pack --profile esm-only && publint",
    "check-size": "size-limit",
    "clean": "rimraf ./dist ./build ./coverage ./_release",
    "dev": "tsdown --watch",
    "fix-staged": "lint-staged --allow-empty",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/jwt-verify.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-bun": "bun --bun run vitest run",
    "test-unit-coverage": "vitest run --coverage",
    "test-unit-edge": "vitest run --environment edge-runtime",
    "test-unit-browser": "vitest run --browser",
    "test-unit-cloudflare-disabled": "vitest run --config ./vitest.cloudflare.config.ts",
    "test-unit-watch": "vitest --ui --api 4515",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-build": "tsc --project tsconfig.build.json --noEmit",
    "ci-coverage-upload": "../../.github/scripts/download/codecov -F httpx-jwt-verify-unit --dir ./coverage"
  },
  "dependencies": {
    "@httpx/lru": "workspace:^",
    "@httpx/plain-object": "workspace:^",
    "jose": "^6.1.3",
    "ky": "^1.14.2"
  },
  "peerDependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@standard-schema/spec": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@azure/identity": "4.13.0",
    "@belgattitude/eslint-config-bases": "8.9.0",
    "@dotenvx/dotenvx": "1.52.0",
    "@httpx/devtools-vitest": "workspace:^",
    "@size-limit/esbuild": "12.0.0",
    "@size-limit/file": "12.0.0",
    "@standard-schema/spec": "1.1.0",
    "aws-jwt-verify": "5.1.1",
    "browserslist": "4.28.1",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "10.1.0",
    "es-check": "9.5.4",
    "eslint": "8.57.1",
    "nock": "14.0.10",
    "npm-run-all2": "8.0.4",
    "prettier": "3.8.1",
    "publint": "0.3.17",
    "rimraf": "6.1.2",
    "size-limit": "12.0.0",
    "tsdown": "0.20.1",
    "tsx": "4.21.0",
    "type-fest": "5.4.1",
    "typedoc": "0.28.16",
    "typedoc-plugin-markdown": "4.9.0",
    "typescript": "5.9.3",
    "valibot": "1.2.0",
    "vitest": "4.1.0-beta.1",
    "zod": "4.3.6"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "directory": "_release/package"
  }
}
