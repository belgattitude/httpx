{
  "entryPoints": [
    "src/guards/*.ts",
    "src/asserts/*.ts",
    "src/types/*.ts",
    "src/helpers/*.ts"
  ],
  "out": "./docs/api/assert",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "disableSources": true,
  "entryDocument": "README.md",
  "githubPages": false,
  "readme": "none",
  "exclude": ["**/*.test.ts"]
}
