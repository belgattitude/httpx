{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "noImplicitAny": false,
    "declaration": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "isolatedModules": true
  }
}
