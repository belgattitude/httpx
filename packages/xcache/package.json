{
  "name": "@httpx/xcache",
  "description": "Memory cache for node and browsers",
  "version": "0.4.11",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "homepage": "https://belgattitude.github.io/httpx/xcache",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/httpx.git",
    "directory": "packages/xcache"
  },
  "keywords": [
    "cache"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "?build-release": "When https://github.com/atlassian/changesets/issues/432 has a solution we can remove this trick",
    "build": "run clean && yarn run tsup",
    "build-release": "yarn build && rimraf ./_release && yarn pack && mkdir ./_release && tar zxvf ./package.tgz --directory ./_release && rm ./package.tgz",
    "bench": "vitest bench --run",
    "bench-bun": "bun --bun run vitest bench --run",
    "bench-codspeed": "cross-env CODSPEED=1 vitest bench --run",
    "bench-watch": "vitest bench",
    "docgen": "run-s docgen-typedoc",
    "docgen-typedoc": "rimraf ./docs/api && typedoc --plugin typedoc-plugin-markdown --out ./docs/api",
    "check-dist": "run-s check-dist-esm check-dist-cjs",
    "check-dist-cjs": "es-check es2022 './dist/**/*.cjs' --not './dist/*.map.js'",
    "check-dist-esm": "es-check es2022 './dist/**/*.mjs' --not './dist/*.map.js' --module",
    "check-pub": "attw --pack && publint",
    "check-size": "size-limit",
    "clean": "rimraf ./dist ./build ./coverage ./_release",
    "dev": "tsup --watch",
    "fix-staged": "lint-staged --allow-empty",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/xcache.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-bun": "bun --bun run vitest run",
    "test-unit-coverage": "vitest run --coverage",
    "test-unit-edge": "vitest run --environment edge-runtime",
    "test-unit-browser": "vitest run --browser",
    "test-unit-cloudflare": "vitest run --config ./vitest.cloudflare.config.ts",
    "test-unit-watch": "vitest --ui --api 4515",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-build": "tsc --project tsconfig.build.json --noEmit",
    "ci-coverage-upload": "../../.github/scripts/download/codecov -F httpx-xcache --dir ./coverage"
  },
  "dependencies": {
    "@httpx/compress": "workspace:^",
    "@httpx/lru": "workspace:^",
    "@httpx/stable-hash": "workspace:^"
  },
  "peerDependencies": {
    "devalue": "^5.4.2",
    "superjson": "^2.2.5"
  },
  "peerDependenciesMeta": {
    "devalue": {
      "optional": true
    },
    "superjson": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@belgattitude/eslint-config-bases": "8.9.0",
    "@httpx/devtools-vitest": "workspace:^",
    "@size-limit/file": "12.0.0",
    "@size-limit/webpack": "12.0.0",
    "browserslist": "4.28.1",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "10.1.0",
    "devalue": "5.6.2",
    "es-check": "9.5.4",
    "esbuild": "0.27.3",
    "eslint": "8.57.1",
    "fflate": "0.8.2",
    "npm-run-all2": "8.0.4",
    "prettier": "3.8.1",
    "pretty-bytes": "7.1.0",
    "publint": "0.3.17",
    "rimraf": "6.1.2",
    "size-limit": "12.0.0",
    "superjson": "2.2.6",
    "tsup": "8.5.1",
    "typedoc": "0.28.17",
    "typedoc-plugin-markdown": "4.10.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "webpack": "5.105.2"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "directory": "_release/package"
  }
}
